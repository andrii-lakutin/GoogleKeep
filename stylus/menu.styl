.sideMenu
	position fixed
	top 45px 
	z-index 2 
	height 100% 
	background #212121 
	width 235px 
	margin-left -240px 
	transition all 750ms cubic-bezier(.87,-.41,.19,1.441)
	box-shadow 4px 2px 9px 0px rgba(0,0,0,0.75)
	overflow hidden
	.cleanAll
		color rgb(247,244,213) 
		text-align center
		width 100%
		height 50px  
		line-height 50px
		cursor pointer
		transition all 400ms
		&:hover
			background rgb(247,244,213) 
			color black 
			width 110% 
		i
			margin-left 20px  
			font-size 22px 
		
		
.showMenu
	margin-left 0px